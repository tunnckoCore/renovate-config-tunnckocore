{
  "name": "renovate-config-tunnckocore",
  "version": "0.4.0",
  "description": "Renovate Shareable config for @tunnckoCore organization",
  "repository": "tunnckoCore/renovate-config-tunnckocore",
  "author": "Charlike Mike Reagent <@tunnckoCore> (https://i.am.charlike.online)",
  "nspId": "todo",
  "scripts": {},
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {},
  "keywords": [
    "renovate",
    "renovate-app",
    "renovateapp",
    "config",
    "shareable",
    "shareable config",
    "shareable-config"
  ],
  "renovate-config": {
    "default": {
      "extends": [
        "config:base",
        ":onlyNpm",
        ":pinVersions",
        ":prNotPending",
        ":prConcurrentLimit20",
        ":automergeMajor",
        ":automergeBranchPush",
        ":automergePr",
        ":automergeLinters",
        ":unpublishSafeDisabled",
        ":maintainLockFilesDisabled",
        ":gitSignOff",
        ":rebaseStalePrs",
        ":semanticCommits",
        ":labels(deps, renovate)",
        ":assignAndReview(charlike)"
      ],
      "prHourlyLimit": 10
    }
  }
}
