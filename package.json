{
  "name": "renovate-config-tunnckocore",
  "version": "0.3.1",
  "description": "Renovate Shareable config for @tunnckoCore organization",
  "repository": "tunnckoCore/renovate-config-tunnckocore",
  "author": "Charlike Mike Reagent <@tunnckoCore> (https://i.am.charlike.online)",
  "nspId": "todo",
  "scripts": {},
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {},
  "keywords": [
    "renovate",
    "renovate-app",
    "renovateapp",
    "config",
    "shareable",
    "shareable config",
    "shareable-config"
  ],
  "renovate-config": {
    "default": {
      "extends": [
        "config:base",
        ":onlyNpm",
        ":pinOnlyDevDependencies",
        ":prNotPending",
        ":automergeMajor",
        ":automergePr",
        ":maintainLockFilesWeekly",
        ":unpublishSafeDisabled"
      ],
      "rebaseStalePrs": true,
      "semanticCommits": true,
      "labels": ["deps", "renovate"],
      "ignoreDeps": ["semantic-release"]
    }
  }
}
